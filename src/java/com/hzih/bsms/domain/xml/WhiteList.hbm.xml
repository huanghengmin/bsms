<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="white_list" name="com.hzih.bsms.domain.WhiteList">
        <id access="field" name="id" type="int" column="id" length="10">
            <generator class="increment"/>
        </id>
        <!--<property name="status" length="10" column="status" access="field" type="int"/>-->
        <property name="url"  column="url" unique="true" access="field" type="string"/>
    </class>
</hibernate-mapping>
